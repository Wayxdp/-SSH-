<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ht.bean">
    <class name="Agency" table="t_agency">
        <id name="id" column="id">
            <generator class="uuid"/>
        </id>
        <property name="name" />
        <property name="email"/>
        <property name="pwd" />
        <property name="leader" />
        <property name="phone" />
        <property name="tel" />
        <property name="address" />
        <property name="logo" />
        <property name="intro" />
        <property name="created_time" />
        <property name="checked_status" />
        <property name="status" />

        <!-- 经销商与楼盘的 一对多关系-->
        <set name="buildings">
            <key column="agency_id"></key>
            <one-to-many class="Buildings"></one-to-many>
        </set>
        <!-- 经销商与员工的 一对多关系-->
        <set name="employees">
            <key column="agency_id"></key>
            <one-to-many class="com.ht.bean.Employee"></one-to-many>
        </set>
        <!--经销商与客户，一对多-->
        <set name="customers">
            <key column="agency_id">
            </key>
            <one-to-many class="Customer"></one-to-many>
        </set>


        <set name="appointments">
            <key column="agency_id"></key>
            <one-to-many class="Agency"></one-to-many>
        </set>

    </class>

</hibernate-mapping>