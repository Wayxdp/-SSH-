<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>


    <package name="activity" namespace="/activity" extends="base-package">
        <action name="*" class="activityController" method="{1}">
            <result name="success" type="json"/>
            <result name="pagerActivity" type="json"/>
            <result name="saveActivity" type="json"/>
            <result name="updateActivity" type="json"/>
            <result name="validActivity" type="json"/>
            <result name="showAlterActivity" >/agency/agency_alterActivity.jsp</result>
            <result name="addActivityImg" type="json"></result>
            <result name="adminActivityDetail" >/admin/admin_activity_detail.jsp</result>
            <result name="adminEditActivity" >/admin/admin_activity_edit.jsp</result>
        </action>
    </package>

</struts>